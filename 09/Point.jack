class Point {
    field int x, y;

    constructor Point new(int Ax, int Ay) {
        let x = Ax;
        let y = Ay;

        return this;
    }

    method int x() {
        return x;
    }

    method int y() {
        return y;
    }

    method void destroy() {
        do Memory.deAlloc(this);
        return;
    }
}
